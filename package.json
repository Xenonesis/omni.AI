  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:production": "vite preview --port 4173 --host",
    "type-check": "tsc --noEmit",
    "test:build": "npm run type-check && npm run lint && npm run build",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy:netlify": "npm run test:build && netlify deploy --prod --dir=dist",
    "setup:production": "node deploy-production.js",
    "verify:api": "node verify-api-connection.js",
    "test:omnidim": "node test-omnidim-endpoints.js",
    "deploy:production": "npm run setup:production && npm run test:build && netlify deploy --prod --dir=dist",
    "start": "node server.js",
    "dev:server": "node server.js",
    "deploy:api": "node deploy-api-server.js",
    "test:performance": "node scripts/performance-test.js",
    "test:performance:dev": "npm run preview & sleep 5 && npm run test:performance && pkill -f 'vite preview'",
    "optimize": "npm run build && npm run test:performance",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release:prepare": "npm run test:build && npm run optimize",
    "release:github": "gh release create v$npm_package_version --generate-notes",
    "docs:build": "typedoc --out docs src",
    "security:audit": "npm audit --audit-level moderate",
    "test:accessibility": "axe-core --dir dist",
    "test:lighthouse": "lighthouse http://localhost:4173 --output json --output-path lighthouse-report.json"
  },
