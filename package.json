{
  "name": "omniverse-ai-voice-marketplace",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:production": "vite preview --port 4173 --host",
    "type-check": "tsc --noEmit",
    "test:build": "npm run type-check && npm run lint && npm run build",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy:netlify": "npm run test:build && netlify deploy --prod --dir=dist",
    "setup:production": "node deploy-production.js",
    "verify:api": "node verify-api-connection.js",
    "test:omnidim": "node test-omnidim-endpoints.js",
    "deploy:production": "npm run setup:production && npm run test:build && netlify deploy --prod --dir=dist",
    "start": "node server.js",
    "dev:server": "node server.js",
    "deploy:api": "node deploy-api-server.js"
  },
  "dependencies": {
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^11.0.5",
    "lucide-react": "^0.344.0",
    "natural": "^6.12.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.1",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
